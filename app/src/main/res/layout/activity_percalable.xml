<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.phgof.android_project.PacelableData.MainPercalable">

    <TextView
        android:id="@+id/tvpacel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="TEXT"
        android:textSize="40sp"
        android:textColor="#000000"
       android:layout_centerHorizontal="true"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Which Sides Are You On?"
        android:layout_below="@id/tvpacel"
        android:textColor="#009624"
        android:textSize="20sp"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/logo_marvel2"
        android:id="@+id/pamarvel"
        android:layout_below="@id/tvpacel"
        android:layout_marginTop="25dp"/>

    <ImageView
        android:id="@+id/pdc"
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/pamarvel"
        android:src="@drawable/logo_dc" />


</RelativeLayout>
